{
    "sublimeTextKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": true,
    "terminal.integrated.fontSize": 12,
    "workbench.statusBar.visible": true,
    "cSpell.language": "en,en-GB",
    "cSpell.userWords": [
        "AASB",
        "GAAP",
        "IFAC",
        "Infographics",
        "Infographs",
        "KPIs",
        "Learnability",
        "PAYG",
        "Paasche's",
        "Serverless",
        "aasb",
        "numpy",
        "overline",
        "strategising"
    ],
    "window.zoomLevel": 1,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, Calibur, Heltevica",
    "update.channel": "default",
    "mdmath.delimiters": "dollars",
    "team.showWelcomeMessage": false,
    "markdown.preview.breaks": true,
    "markdown.preview.doubleClickToSwitchToEditor": true,
    "markdown.preview.scrollEditorWithPreview": true,
    "markdown.preview.scrollPreviewWithEditor": true,
    "markdown-preview-enhanced.pandocPath": "pandoc",
    "markdown-toc.updateOnSave": true,
    "markdown-toc.withLinks": true,
    "markdown-preview-enhanced.latexEngine": "pdflatex",
    "markdown-preview-enhanced.pandocMarkdownFlavor": "markdown-raw_tex+tex_math_single_backslash",
    "python.autoComplete.addBrackets": true,
    "python.autoComplete.preloadModules": [],
    "python.pythonPath": "/anaconda3/bin/python",
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#3083b4",
        "statusBar.noFolderBackground": "#3083b4",
        "statusBar.debuggingBackground": "#3083b4",
    // Git Control
    "git.enable": true,
    "git.path": "/usr/local/bin/git",
    },
    "scm.alwaysShowProviders": true,
    "files.associations": {
        "*.md": "markdown",
        "*.py":"python",
    },
    "git.autofetch": true
}